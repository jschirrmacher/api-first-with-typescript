{
  "all": true,
  "extension": [
    ".ts",
    ".js"
  ],
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
  ],
  "reporter": [
    "html",
    "text-summary"
  ],
  "report-dir": "test-results/coverage",
  "watermarks": {
    "lines": [60, 80],
    "functions": [60, 80],
    "branches": [60, 80],
    "statements": [60, 80]
  }
}
